<!-- Portfolio Grid Section -->
<section id="portfolio" class="bg-light-gray">
  <div class="container">
    <div class="row">
      <center><img src="img/logos/cartelogo.jpg" alt="logo" height="190" width="140" style="position: absolute; top: 60px; left: 550px;"</center>
      <div class="col-lg-12 text-center">
        <h2 class="section-heading">Your Stops</h2>
        <h3 class="section-subheading text-muted">We found these amazing places that you need to stop and try on the way to your destination!</h3>
      </div>
    </div>

    <div class="row">
    <%_.each(restaurants, function(restaurant) { %>
      <div class="col-md-4 col-sm-6 portfolio-item">
        <a href="<%= restaurant.url %>" class="portfolio-link" data-toggle="modal">
          <div class="portfolio-hover">
            <div class="portfolio-hover-content">
              <i class="fa fa-plus fa-3x"></i>
            </div>
          </div>
          <img src="<%= restaurant.image_url %>" width="400" height="289" class="img-responsive" alt="">
        </a>
        <div class="portfolio-caption">
          <h4><%= restaurant.name %></h4>
          <p class="text-muted"><%= restaurant.location.city %></p>
          <img src="<%= restaurant.rating_img_url %>" align="middle">
        </div>
      </div>
    <% }) %>
    </div>

  </div>
</section>

<!-- sponsor Aside -->
<aside class="sponsor">
  <div class="container">
    <div class="center">
      <a href="#">
        <img src="img/logos/yelp.jpg" align="right" alt="logo">
      </a>
    </div>
  </div>
</aside>


